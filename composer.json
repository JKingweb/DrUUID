{
    "name":        "jkingweb/druuid",
    "description": "DrUUID RFC 4122 UUID library for PHP",
    "keywords":    ["uuid"],
    "license":     "MIT",
    "authors":     [
        {
            "name":     "J. King",
            "homepage": "http://jkingweb.ca/"
        }
    ],
    "support": {
        "issues": "https://github.com/JKingweb/DrUUID/issues",
        "source": "https://github.com/JKingweb/DrUUID",
        "docs":   "http://jkingweb.ca/code/php/lib.uuid/"
    },
    "require":     {
        "php": ">=5.3.0"
    },
    "suggest": {
        "phpseclib/phpseclib": "Use on 32-bit systems without GMP or BC Math"
    },
    "autoload":    {
        "psr-4": {
            "jkingweb\\uuid": "src/"
        }
    }
}